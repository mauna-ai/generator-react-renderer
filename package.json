{
  "name": "@psychobolt/generator-react-renderer",
  "version": "0.0.4",
  "description": "Yeoman generator for scaffolding extensible ES6 React Fiber renderers",
  "main": "generators/app/index.js",
  "repository": "https://github.com/psychobolt/generator-react-renderer",
  "author": "psychobolt",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install -s -o",
    "build": "rimraf generators && babel src -d generators --copy-files --source-maps",
    "start": "npm run build",
    "watch": "npm start -- -- -w",
    "lint": "eslint src *.js"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "fbjs": "^0.8.17",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-reconciler": "^0.12.0",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "cash-true": "^0.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.74.0",
    "flow-typed": "^2.4.0",
    "rimraf": "^2.6.2",
    "yo": "^2.0.2"
  }
}
